# Development Environment Configuration
# Copy this file to .env and update with your values

# Required for EC2 deployment
KEY_PAIR_NAME=your-ec2-key-pair-name

# Deployment configuration
DEPLOYMENT_ENV=development
DEPLOYMENT_MODE=EC2

# Optional configurations
ALERT_EMAIL=your-email@example.com
ALLOWED_IPS=0.0.0.0/0

# Domain configuration (optional)
# DOMAIN_NAME=dev.example.com
# CERTIFICATE_ARN=arn:aws:acm:region:account:certificate/xxx
# HOSTED_ZONE_ID=Z1234567890ABC

# VPC configuration (optional - use existing VPC)
# EXISTING_VPC_ID=vpc-1234567890abcdef0

# Features
ENABLE_RAG=true
ENABLE_MEILISEARCH=false
ENABLE_SHAREPOINT=false